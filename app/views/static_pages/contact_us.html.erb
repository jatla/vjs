<%= render 'content' %>
<%= form_for(@admins_message) do |f| %>
  <div class="field">
    <%= f.label :subject %><br>
    <%= f.text_field :subject%>
  </div>

  <div class="textarea">
    <%= f.label :message %><br>
    <%= f.text_area :message, options = {length: 40} %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>